{
  "name": "teach-honcho",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "biome check .",
    "lint": "biome check --write --unsafe .",
    "check:write": "biome check --write .",
    "dev": "next dev --turbo",
    "preview": "next build && next start",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "precommit": "pnpm run typecheck && pnpm run lint",
    "clean:chat": "tsx cli/cleanChat.ts",
    "batch:clean": "tsx cli/batchClean.ts",
    "analyze:chat": "tsx src/analyzeCleanJson.ts",
    "upload:chat": "dotenvx run -- tsx cli/uploadChat.ts",
    "batch:upload": "dotenvx run -- tsx cli/batchUpload.ts",
    "get:representation": "dotenvx run -- tsx cli/getRepresentation.ts"
  },
  "dependencies": {
    "@honcho-ai/sdk": "^1.4.0",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-collapsible": "^1.1.11",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@t3-oss/env-nextjs": "^0.12.0",
    "@tanstack/react-query": "^5.69.0",
    "@trpc/client": "^11.0.0",
    "@trpc/next": "^11.0.0",
    "@trpc/react-query": "^11.0.0",
    "@trpc/server": "^11.0.0",
    "clsx": "^2.1.1",
    "next": "^15.2.3",
    "next-themes": "^0.4.6",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.61.1",
    "superjson": "^2.2.1",
    "tailwind-merge": "^3.3.1",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@biomejs/biome": "2.1.3",
    "@dotenvx/dotenvx": "^1.48.4",
    "@tailwindcss/postcss": "^4.0.15",
    "@types/node": "^20.14.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "dotenv": "^17.2.1",
    "postcss": "^8.5.3",
    "tailwindcss": "^4.0.15",
    "ts-node": "^10.9.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.2"
